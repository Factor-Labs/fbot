{
    "name": "One Factor",
    "version": "1.1",
    "description": "Intelligent Automation from Factor Labs",
    "permissions":[
        "storage",
        "contextMenus"
    ],
    "browser_action": {
	    "default_icon": {
		    "16": "images/fbot_one16.png",
		    "32": "images/fbot_one32.png",
		    "128": "images/fbot_one128.png"
	    },
	    "default_title": "Factor"
    },
    "icons": {
        "16": "images/fbot_one16.png",
        "32": "images/fbot_one32.png",
        "128": "images/fbot_one128.png"
    },
    "background": {
        "scripts":["background.js"],
        "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
}
